{
  "name": "honeydew-website",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/prebuild.js",
    "build": "npm run prebuild && tsc --incremental --pretty false && vite build && npm run prerender",
    "prerender": "node scripts/prerender-seo.js",
    "build:qa": "npm run prebuild && tsc --incremental --pretty false && vite build --mode qa --outDir dist-qa",
    "start": "npx serve dist -p $PORT",
    "preview": "vite preview",
    "schedule-content": "node scripts/schedule-content.js",
    "ga4-setup": "node scripts/ga4-cli.js setup-funnel --property=properties/465457170",
    "seo-check": "node scripts/seo-check.js",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-blog-manifest": "node scripts/generate-blog-manifest.js",
    "index-build": "node scripts/index-build.js",
    "index-build:qa": "node scripts/index-build.js dist-qa",
    "test": "npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "ads:setup": "node scripts/google-ads-cli.js setup",
    "ads:optimize": "node scripts/ads-optimizer.js run",
    "ads:daemon": "node scripts/ads-optimizer.js daemon",
    "ads:report": "node scripts/ads-optimizer.js report",
    "ads:status": "node scripts/google-ads-cli.js status",
    "gsc:wow": "node scripts/gsc-cli.js week-over-week --property=https://www.gethoneydew.app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Honeydew - Modern Task Management Website",
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.81.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "drizzle-zod": "^0.8.2",
    "framer-motion": "^12.19.1",
    "googleapis": "^133.0.0",
    "lucide-react": "^0.523.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.58.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "serve": "^14.2.1",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1"
  }
}
